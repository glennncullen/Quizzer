@model IEnumerable<Quizzer.Models.HistoryQuestion>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.createdBy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.question)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.answerOne)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.answerTwo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.answerThree)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.answerFour)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.correctAnswer)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.createdBy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.question)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.answerOne)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.answerTwo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.answerThree)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.answerFour)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.correctAnswer)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
